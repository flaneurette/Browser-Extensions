{
	"manifest_version": 3,
	"name": "Quick Tab Cleaner",
	"version": "1.1",
	"description": "Find and close inactive tabs quickly.",
	"permissions": ["tabs"],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"16": "icon.png",
			"48": "icon.png",
			"128": "icon.png"
		}
	},
	"web_accessible_resources": [{
		"resources": ["about.html"],
		"matches": ["<all_urls>"]
	}]
}
